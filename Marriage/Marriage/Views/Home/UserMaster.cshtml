<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
     <link href="../../Content/Marriagestyle.css" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.2/themes/smoothness/jquery-ui.css" />
    <script type="text/javascript" src="http://code.jquery.com/jquery-1.9.1.js"></script>
    <script "text/javascript" src="http://code.jquery.com/ui/1.10.2/jquery-ui.js"></script>
</head>
<body>
    <div class="page">
        <div id="header">
            <div id="title">
                <h1>Marriageexpress.in</h1>
            </div>
            <div id="logindisplay">
                @Html.Partial("Logoff")
            </div>
            <div id="menucontainer">
           
                <ul id="menu">
                    <li>@Html.ActionLink("Welcome", "Memberlist", "Home")</li>
                    <li>@Html.ActionLink("Profile", "Profile", "Home")</li>
                </ul>
            </div>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
        </div>
    </div>
</body>
</html>
